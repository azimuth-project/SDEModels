package org.azimuthproject.constant;

/**
 * This class provides mathematical constants as static variables
 * @author Tim van Beek
 */
public final class MathConstant
{

	/**
	 * The Euler number e.
	 */
	public static final double EULER_CONSTANT = 2.71828182845904523536028747135;

	/**
	 * the number Pi
	 */
	public static final double PI = 3.14159265358979323846264338328; /* pi */

	/**
	 * private constructor, this class should not be instantiated
	 */
	private MathConstant()
	{
		// nothing to do here 
	}
}
